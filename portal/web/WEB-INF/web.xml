<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login_request</servlet-name>
        <servlet-class>servelt_classes.login_request</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_project</servlet-name>
        <servlet-class>servelt_classes.add_project</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>edit_project</servlet-name>
        <servlet-class>servelt_classes.edit_project</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>remove_doc</servlet-name>
        <servlet-class>servelt_classes.remove_doc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>del_project</servlet-name>
        <servlet-class>servelt_classes.del_project</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_request</servlet-name>
        <url-pattern>/login_request</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_project</servlet-name>
        <url-pattern>/add_project</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>edit_project</servlet-name>
        <url-pattern>/edit_project</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>remove_doc</servlet-name>
        <url-pattern>/remove_doc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>del_project</servlet-name>
        <url-pattern>/del_project</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
